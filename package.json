  "scripts": {
    "cy:run": "cypress run",
    "cy:parallel": "cypress-parallel -s cy:run -t 32 -d cypress/e2e -a '\"--config baseUrl=http://localhost:5432/\"'",
    "dev": "next dev -p 5432",
    "dev:e2e": "cypress open",
    "dev:multiplayer": "cd socket_relay_server && npm start",
    "dev:all": "concurrently --names 'WEB_UI,E2E_TESTS,MULTIPLAYER_SERVER' --kill-others 'npm run dev' 'npm run dev:e2e' 'npm run dev:multiplayer'",
